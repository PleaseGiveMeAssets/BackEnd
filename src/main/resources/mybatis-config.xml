<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
        <setting name="logImpl" value="SLF4J"/>
    </settings>

    <typeAliases>
        <typeAlias type="com.example.spring.domain.RecommendStock" alias="RecommendStock"/>
        <typeAlias type="com.example.spring.domain.Stock" alias="Stock"/>
        <typeAlias type="com.example.spring.domain.Portfolio" alias="Portfolio"/>
        <typeAlias type="com.example.spring.domain.Notification" alias="Notification"/>
        <typeAlias type="com.example.spring.domain.User" alias="User"/>
        <typeAlias type="com.example.spring.domain.DailyReport" alias="DailyReport"/>
        <typeAlias type="com.example.spring.domain.Question" alias="Question"/>
        <typeAlias type="com.example.spring.domain.QuestionOption" alias="QuestionOption"/>
        <typeAlias type="com.example.spring.vo.UserAnswerVO" alias="UserAnswerVO"/>
        <typeAlias type="com.example.spring.vo.StockVO" alias="StockVO"/>
        <typeAlias type="com.example.spring.domain.SavedNews" alias="SavedNews"/>
        <typeAlias type="com.example.spring.dto.FindIdRequestDTO" alias="FindIdRequestDTO"/>
        <typeAlias type="com.example.spring.dto.ForChartDTO" alias="ForChartDTO"/>
        <typeAlias type="com.example.spring.dto.FindPasswordRequestDTO" alias="FindPasswordRequestDTO"/>
        <typeAlias type="com.example.spring.domain.SelectedTerms" alias="SelectedTerms"/>
        <typeAlias type="com.example.spring.domain.SavedReport" alias="SavedReport"/>
        <typeAlias type="com.example.spring.dto.MemberDTO" alias="MemberDTO"/>
        <typeAlias type="com.example.spring.domain.TermsOfUse" alias="TermsOfUse"/>
        <typeAlias type="com.example.spring.domain.StockHistory" alias="StockHistory"/>
    </typeAliases>

    <mappers>
        <mapper resource="mapper/DailyReportMapper.xml"/>
        <mapper resource="mapper/InterestCategoryMapper.xml"/>
        <mapper resource="mapper/NotificationMapper.xml"/>
        <mapper resource="mapper/PortfolioMapper.xml"/>
        <mapper resource="mapper/QuestionMapper.xml"/>
        <mapper resource="mapper/SavedNewsMapper.xml"/>
        <mapper resource="mapper/SavedReportMapper.xml"/>
        <mapper resource="mapper/SearchMapper.xml"/>
        <mapper resource="mapper/StockMapper.xml"/>
        <mapper resource="mapper/UserAnswerMapper.xml"/>
        <mapper resource="mapper/UserMapper.xml"/>
        <mapper resource="mapper/InterestCategoryMapper.xml"/>
        <mapper resource="mapper/ProfileEditMapper.xml"/>
        <mapper resource="mapper/TermsOfUseMapper.xml"/>
        <mapper resource="mapper/DailyReportMapper.xml"/>
        <mapper resource="mapper/StockHistoryMapper.xml"/>
        <mapper resource="mapper/MarketHolidayMapper.xml"/>
        <mapper resource="mapper/SearchDetailStockMapper.xml"/>
    </mappers>
</configuration>
