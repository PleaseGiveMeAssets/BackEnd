<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.spring.mapper.QuestionMapper">

    <select id="selectSurveyQuestion" parameterType="int" resultType="com.example.spring.vo.QuestionVO">
        SELECT QUESTION_ID as questionId, CONTENT as content
        FROM QUESTION
        WHERE QUESTION_ID = #{questionId}
    </select>


</mapper>
